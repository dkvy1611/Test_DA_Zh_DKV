    "null_count = data['CustomerID'].isnull().sum()\n",
    "total_count = data.shape[0]\n",
    "null_ratio = null_count / total_count\n",
    "\n",
    "# In tỉ lệ null\n",
    "print(f\"Ratio of missing CustomerID: {null_ratio:.2%}\")"
